            "angle": 1
        },
        {
            "func": "destroy",
            "wait": 2
        }
    ],
    "evaluate": [
        {
            "type": "states",
            "key": "_state_containers.models._models.(1,)._atoms_drawing._draw_shape.bindings._opengl_context.window.view._background_rgba",
