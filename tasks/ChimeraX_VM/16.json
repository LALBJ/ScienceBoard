{
    "type": "ChimeraX",
    "sort": "VM",
    "steps": 5,
    "instruction": "Detect all clashes with VDW overlap ≥ 0.3Å in ChimeraX.",
    "version": "0.1",
    "snapshot": "sci_bench",
    "initialize": [
        {
            "func": "execute",
            "command": [
                "rm",
                "/home/user/.local/share/ChimeraX/commands",
                "/home/user/.local/share/ChimeraX/file_history",
                "/home/user/.local/share/ChimeraX/preregistration"
            ]
        },
        {
            "func": "launch",
            "wait": 5,
            "command": [
                "flatpak",
                "run",
                "edu.ucsf.rbvi.ChimeraX",
                "--cmd",
                "remotecontrol rest start json true port «PORTLIKE»"
            ]
        },
        {
            "func": "alphafold_match",
            "name": "A8Z1J3"
        },
        {
            "func": "turn",
            "axis": "y",
            "angle": 1
        },
        {
            "func": "color",
            "style": "bypolymer"
        },
        {
            "func": "destroy"
        }
    ],
    "evaluate": [
        {
            "type": "states",
            "find": "lambda key, value: key.endswith('_name') and value == 'clashes'",
            "key": "lambda key: key",
            "pattern": ".+"
        },
        {
            "type": "states",
            "find": "lambda key, value: key.endswith('page_source')",
            "key": "lambda key: key",
            "pattern": "4 clashes"
        }
    ]
}
