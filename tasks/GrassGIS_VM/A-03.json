{
    "type": "GrassGIS",
    "sort": "VM",
    "steps": 5,
    "instruction": "隐藏 Map Display 1 的 Toolbars",
    "version": "0.1",
    "snapshot": "sci_bench",
    "initialize": [
        {
            "func": "execute",
            "command": "rm -rf /home/user/grassdata/*/*/.gislock",
            "shell": true
        },
        {
            "func": "launch",
            "wait": 5,
            "command": "gnome-terminal -- /bin/bash -ic \"FLASK_PORT=8000 grass --gui /home/user/grassdata/world_latlong_wgs84/PERMANENT\"",
            "shell": true
        },
        {
            "func": "map",
            "grassdb": "/home/ichinoe/grassdata",
            "location": "natural_earth_dataset",
            "mapset": "PERMANENT"
        }
    ],
    "evaluate": [
        {
            "type": "info",
            "key": "lambda dump: dump['toolbars']['shown']",
            "value": false
        }
    ]
}