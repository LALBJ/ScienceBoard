{
    "type": "GrassGIS",
    "sort": "VM",
    "steps": 5,
    "instruction": "将中心点移动到东经 25°、北纬 20°",
    "version": "0.1",
    "snapshot": "sci_bench",
    "initialize": [
        {
            "func": "execute",
            "command": "rm -rf /home/user/grassdata/*/*/.gislock",
            "shell": true
        },
        {
            "func": "launch",
            "wait": 5,
            "command": "gnome-terminal -- /bin/bash -ic \"FLASK_PORT=8000 grass --gui /home/user/grassdata/world_latlong_wgs84/PERMANENT\"",
            "shell": true
        },
        {
            "func": "map",
            "grassdb": "/home/ichinoe/grassdata",
            "location": "natural_earth_dataset",
            "mapset": "PERMANENT"
        },
        {
            "func": "layer",
            "wait": 5,
            "query": {
                "name": "countries"
            }
        }
    ],
    "evaluate": [
        {
            "type": "info",
            "key": "lambda dump: dump['region']['center_easting']",
            "value": 25,
            "pred": "lambda key, value: abs((key % 360) - value) < 1"
        },
        {
            "type": "info",
            "key": "lambda dump: dump['region']['center_northing']",
            "value": 20,
            "pred": "lambda key, value: abs(key - value) < 1"
        }
    ]
}