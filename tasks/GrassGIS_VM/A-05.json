{
    "type": "GrassGIS",
    "sort": "VM",
    "steps": 5,
    "instruction": "将 Statusbar 切换到 Display geometry",
    "version": "0.1",
    "snapshot": "sci_bench",
    "initialize": [
        {
            "func": "execute",
            "command": "rm -rf /home/user/grassdata/*/*/.gislock",
            "shell": true
        },
        {
            "func": "launch",
            "wait": 5,
            "command": "gnome-terminal -- /bin/bash -ic \"FLASK_PORT=8000 grass --gui /home/user/grassdata/world_latlong_wgs84/PERMANENT\"",
            "shell": true
        },
        {
            "func": "map",
            "grassdb": "/home/user/grassdata",
            "location": "natural_earth_dataset",
            "mapset": "PERMANENT"
        },
        {
            "func": "layer",
            "wait": 5,
            "query": {
                "name": "countries"
            }
        }
    ],
    "evaluate": [
        {
            "type": "info",
            "key": "lambda dump: dump['statusbar']['mode']",
            "value": 3
        }
    ]
}