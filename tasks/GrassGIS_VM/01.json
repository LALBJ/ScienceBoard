{
    "type": "GrassGIS",
    "sort": "VM",
    "steps": 5,
    "instruction": "Display and ONLY display the layer of 'boundary_region' in Grass GIS.",
    "version": "0.1",
    "snapshot": "sci_bench",
    "initialize": [
        {
            "func": "launch",
            "wait": 5,
            "command": "gnome-terminal -- /bin/bash -ic \"FLASK_PORT=8000 grass --gui /home/user/grassdata/world_latlong_wgs84/PERMANENT\"",
            "shell": true
        },
        {
            "func": "map",
            "grassdb": "/home/user/grassdata",
            "location": "nc_basic_spm_grass7",
            "mapset": "PERMANENT"
        },
        {
            "func": "layer",
            "query": {
                "name": "basins"
            }
        }
    ],
    "evaluate": [
        {
            "type": "info",
            "key": "lambda dump: len(dump['layers'])",
            "value": 1
        },
        {
            "type": "info",
            "key": "lambda dump: dump['layers'][0]['name']",
            "value": "boundary_region@PERMANENT"
        }
    ]
}