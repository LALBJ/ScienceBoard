{
    "type": "GrassGIS",
    "sort": "VM",
    "steps": 5,
    "instruction": "输出当前地图的元数据（metadata）",
    "version": "0.1",
    "snapshot": "sci_bench",
    "initialize": [
        {
            "func": "execute",
            "command": "rm -rf /home/user/grassdata/*/*/.gislock",
            "shell": true
        },
        {
            "func": "launch",
            "wait": 5,
            "command": "gnome-terminal -- /bin/bash -ic \"FLASK_PORT=8000 grass --gui /home/user/grassdata/world_latlong_wgs84/PERMANENT\"",
            "shell": true
        },
        {
            "func": "map",
            "grassdb": "/home/ichinoe/grassdata",
            "location": "grassdata_piemonte_utm32n_wgs84_grass7",
            "mapset": "PERMANENT"
        },
        {
            "func": "layer",
            "wait": 5,
            "query": {
                "name": "boundary_region"
            }
        }
    ],
    "evaluate": [
        {
            "type": "info",
            "key": "lambda dump: dump['output']",
            "value": "v.info map=boundary_region@PERMANENT",
            "pred": "lambda key, value: value in key"
        }
    ]
}